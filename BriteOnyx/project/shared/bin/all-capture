#!/usr/bin/env bash
# Intended to be executed in a Bash shell.
[[ -n "${BO_Trace}" ]] && 1>&2 log_trace "Executing ${BASH_SOURCE}" && [[ "${BO_Trace}" != 'TRACE' ]] && set -vx
# NO: set -e
# NO: trap ... EXIT
###############################################################################
[[ -z "${BO_Project}" ]] &&
    1>&2 log_error "Aborting, this project is NOT ACTIVATED" &&
    exit 99

main() {
    # Capture all project configuration status
    require_arguments $# 0

    anaconda-capture
    env-capture
    py-capture
    tool-capture

    return 0
}

main $@

