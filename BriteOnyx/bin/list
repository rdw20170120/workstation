#!/usr/bin/env python3
# NOTE: Intended to be executed directly by the user.

""" Print list of files and directories within current directory, recursively."""

from os import walk

def main():
   for aRoot, aDirectories, aFiles in walk("."):
      print("Directory '%s'" % aRoot)
      for aFile in aFiles:
         print("File '%s/%s'" % (aRoot, aFile))

if __name__ == "__main__":
   main()

