#!/usr/bin/env python3
"""Print listing of current directory, recursively.

Intended to be executed directly by the user.
"""

from os import walk


def main():
   for aRoot, aDirectories, aFiles in walk("."):
      print("Directory '%s'" % aRoot)
      for aFile in aFiles:
         print("File '%s/%s'" % (aRoot, aFile))

if __name__ == "__main__":
   main()

