#!/usr/bin/env bash
set -e
# DISABLED: set -x
# Intended to be executed directly by the user.
###############################################################################
# Capture BASH environment status

if [[ -z "$BO_Project" ]] ; then
    echo "FATAL: Environment variable 'BO_Project' is undefined, aborting"
    exit 1
fi

File=$BO_Project/out/env.out

echo "INFO: Capturing BASH environment status to '$File'"
env-report > $File

# Capture activation output
cp $BO_Project/*.env $BO_Project/out

###############################################################################
: << 'DisabledContent'
DisabledContent

