#!/usr/bin/env bash
set -e
# DISABLED: set -x
# Intended to be executed directly by the user.
###############################################################################
# Compile all Python source

if [[ -z "$BO_Project" ]] ; then
    echo "FATAL: Environment variable 'BO_Project' is undefined, aborting"
    exit 1
fi

echo "INFO: Compiling Python source files..."
python3 -m compileall -f $BO_Project/BriteOnyx/bin
python3 -m compileall -f $BO_Project/bin
python3 -m compileall -f $BO_Project/src/app
python3 -m compileall -f $BO_Project/src/gen
python3 -m compileall -f $BO_Project/src/lib

###############################################################################
: << 'DisabledContent'
DisabledContent

