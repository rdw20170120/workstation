#!/usr/bin/env bash
set -e
# DISABLED: set -x
# Intended to be executed directly by the user.
###############################################################################
# Check BASH environment status

if [[ -z "$BO_Project" ]] ; then
    echo "FATAL: Environment variable 'BO_Project' is undefined, aborting"
    exit 1
fi

File=$BO_Project/out/env.out

if [[ ! -r "$File" ]] ; then
    echo "FATAL: BASH environment status file '$File' is unreadable, aborting"
    exit 1
fi

echo "INFO: Checking BASH environment status against '$File'"
env-report | diff - $File

###############################################################################
: << 'DisabledContent'
DisabledContent

