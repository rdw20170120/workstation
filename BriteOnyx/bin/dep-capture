#!/usr/bin/env bash
set -e
# DISABLED: set -x
# Intended to be executed directly by the user.
###############################################################################
# Capture Python dependency status

if [[ -z "$BO_Project" ]] ; then
    echo "FATAL: Environment variable 'BO_Project' is undefined, aborting"
    exit 1
fi

File=$BO_Project/cfg/requirements.txt

echo "INFO: Capturing Python dependency status to '$File'"
dep-report > $File

###############################################################################
: << 'DisabledContent'
DisabledContent

