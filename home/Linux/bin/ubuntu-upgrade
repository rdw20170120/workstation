#!/usr/bin/env bash
# Intended to be executed in a Bash shell.
[[ -n "${BO_Trace}" ]] && 1>&2 echo "Executing ${BASH_SOURCE}" && [[ "${BO_Trace}" != 'TRACE' ]] && set -vx
# NO: set -e
# NO: trap ... EXIT
###############################################################################

main() {
    # Upgrade packages installed on the Ubuntu operating system

    sudo apt-get upgrade --assume-yes --quiet

    return 0
}

main $@

