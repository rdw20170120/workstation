Host MeAtBitBucket
    Compression yes
    HostName bitbucket.org
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking yes
    User git

Host MeAtGitHub
    Compression yes
    HostName github.com
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking yes
    User git

Host MeAtGitLab
    Compression yes
    HostName gitlab.com
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking yes
    User git

