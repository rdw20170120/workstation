# Here is how to test these (for example)
# ssh -T git@github.com
# ssh -T git@gitlab.com
# Type 'yes' and hit 'Enter' to accept the authenticity of the GitHub host machine.
# ssh -T MeAtGitHub
# ssh -T MeAtGitLab

Host MeAtGitHub
    Compression no
    HostName github.com
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking yes
    User git

Host MeAtGitLab
    Compression no
    HostName gitlab.com
    IdentityFile ~/.ssh/id_rsa
    StrictHostKeyChecking yes
    User git

Host DevAtAws
    Compression no
    HostName ec2-18-222-254-236.us-east-2.compute.amazonaws.com
    IdentityFile ~/.ssh/rob-dev-workstation.pem
    StrictHostKeyChecking yes
    User ubuntu

