#!/usr/bin/env bash
# This script is intended to be executed by a user in a Bash shell.

# Clone git repositories, if they have not been cloned already.
# Clone from GitHub from others' accounts
# NOTE: I reference my special SSH configuration to handle the connection.
# TODO: Implement this:
# hint:   git config pull.ff only       # fast-forward only

source ${HOME}/bin/lib/declare-git.bash

_Source=GitHub
_DirBase=${HOME}/repo/other
_UrlPrefix=MeAt${_Source}

_Name=earthly
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} ${_Name}

_Name=elves
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} awesome-elvish
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} elvish

_Name=jorgebucaran
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} fisher
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} fishtape

_Name=zzamboni
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} dot-elvish
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} dot-emacs
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} elvish-completions
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} elvish-modules
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} elvish-themes
maybe_clone "${_DirBase}/${_Name}" ${_UrlPrefix}:${_Name} emacs-org-leanpub

: << 'DisabledContent'
DisabledContent
